var cities : [String: String] = [ "Almaty": "Аla" , "Aralsk" : "ARL" , "Aqtobe" :"AKT" , "Tulkibas" : "TLB"]
for (key,value) in cities {
    print("\(key):\(value)")
 if  key == "Aralsk" {
    print("Я нашел свой город!")
}
}
// Задание 6
var movementHeartRates = ["Прыжки со скакалкой": 78, "Бег на беговой дорожке": 89, "Приседание": 80]
var lowHR = 79
var highHR = 91

for (key, value) in movementHeartRates {
    print("\(key):\(value)")  
    
    if value >= lowHR {
        print("Вы могли бы сделать \(key).")
    }
}
