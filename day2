    
  }// Задание 4
let cadence = 60
var testSteps = 0 
import Foundation 
repeat {
    print("Сделайте шаг")
    testSteps += 1
 Thread.sleep(forTimeInterval: 60.0 / Double(cadence))
} while testSteps < 10

